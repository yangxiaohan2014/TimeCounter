<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:timeCountView="http://schemas.android.com/com.yangxiaohan.timecountview"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.yangxiaohan.timecountview.MainActivity">

    <com.yangxiaohan.timecountview.TimeCountView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        timeCountView:textColor="@android:color/holo_blue_bright"
        timeCountView:textSize="16sp"
        android:layout_alignParentStart="true" />
</RelativeLayout>
